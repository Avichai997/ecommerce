{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "Ecommerce store project",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch . --exec babel-node server.js",
    "debug": "ndb -r @babel/register server.js",
    "seed": "babel-node Seed/mockData/importDataToDB.js",
    "build": "npm i --force && rimraf build && ncc build server.js -o build && rimraf build/code-points.mem",
    "serve": "node build/index.js",
    "heroku-prebuild": "cd client && npm install --dev",
    "merge": "git pull origin main && git merge main"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.7",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "ndb": "^1.1.5",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1"
  },
  "engines": {
    "node": "v19.8.1",
    "npm": "9.6.6"
  }
}
